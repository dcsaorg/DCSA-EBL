{
  "required": [
    "URL",
    "partyName",
    "publicKey"
  ],
  "type": "object",
  "description": "refers to an eBL Party solution provider\n",
  "allOf": [
    {
      "type": "object",
      "properties": {
        "partyName": {
          "allOf": [
            {
              "$ref": "partyName.json"
            },
            {
              "description": "Name of the eBL solution provider\n"
            },
            {
              "example": "TradeLens"
            }
          ]
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "LEI": {
          "maxLength": 20,
          "minLength": 20,
          "type": "string",
          "description": "Legal Entity Identifier for the ebl solution provider\n",
          "example": "984500EF1C69E11F2F23"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "publicKey": {
          "$ref": "publicKey.json"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "URL": {
          "type": "string",
          "description": "A Uniform Resource Locator pointing to the solution provider\n",
          "example": "https://www.tradelens.com"
        }
      }
    }
  ],
  "$schema": "http://json-schema.org/schema#"
}