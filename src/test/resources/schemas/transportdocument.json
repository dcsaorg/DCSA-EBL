{
  "required": [
    "shippingInstruction"
  ],
  "type": "object",
  "description": "The document that governs the terms of carriage between shipper and carrier for maritime transportation. Two distinct types of transport documents exist:\n- Bill of Lading\n- Sea Waybill. \n",
  "allOf": [
    {
      "$ref": "transportDocumentRoot.json"
    },
    {
      "type": "object",
      "properties": {
        "placeOfIssue": {
          "allOf": [
            {
              "$ref": "location.json"
            },
            {
              "example": {
                "locationName": "Eiffel Tower",
                "latitude": "48.8585500",
                "longitude": "2.294492036",
                "UNLocationCode": "FRPAR"
              }
            },
            {
              "description": "The location where the original transport document (bill of lading) will be or has been issued.\n"
            }
          ]
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "shippingInstruction": {
          "allOf": [
            {
              "$ref": "shippingInstructionResponse.json"
            }
          ]
        }
      }
    },
    {
      "$ref": "charges.json"
    },
    {
      "$ref": "carrierClauses.json"
    }
  ],
  "$schema": "http://json-schema.org/schema#"
}